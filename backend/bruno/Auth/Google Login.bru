meta {
  name: Google Login
  type: http
  seq: 8
}

post {
  url: {{baseUrl}}/auth/google
  body: json
  auth: none
}

body:json {
  {
    "idToken": "REPLACE_WITH_VALID_GOOGLE_ID_TOKEN", 
    "deviceId": "device_google_1"
  }
}

vars:pre-request {
  baseUrl: http://localhost:8080
}

script:post-response {
  if (res.status === 200) {
      bru.setVar("accessToken", res.body.accessToken);
  }
}
